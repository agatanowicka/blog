<%- include('layout/header.ejs'); -%>
<div class=homePage>
<div class="parallaxHome">
    <h1 class="parallax3Heading"> Us travellers </h1>
</div>
<div class="row allUsers">
    <% for( let i=0; i< allUsers.length; i++){ %>
    <div class="col-sm-6">
        <div class="card userCard">
            <div class="card-body " >
                <h5 class="card-title homeCardTitle"><%=allUsers[i].firstName %> <%= allUsers[i].lastName %></h5>
                <a href="/home/<%=allUsers[i]._id %>" class="btn btn-success btn-lg">View profile</a>
            </div>
        </div>
    </div>
    <% }; %>
</div>
</div>
<%- include('layout/footer.ejs'); -%>

